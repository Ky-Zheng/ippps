{
  "name": "ui",
  "version": "0.0.0",
  "main": "./index.tsx",
  "types": "./index.tsx",
  "scripts": {
    "build": "tsup index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./styles/global.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup index.tsx --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./styles/global.css -o ./dist/styles.css --watch\"",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf coverage"
  },
  "peerDependencies": {
    "next": "^13.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.7",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "config": "*",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "postcss": "^8.4.18",
    "react": "^18.2.0",
    "tailwindcss": "^3.2.1",
    "tsconfig": "*",
    "tsup": "^6.3.0",
    "typescript": "^4.5.2"
  }
}